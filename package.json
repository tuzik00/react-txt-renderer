{
  "name": "react-txt-rendere",
  "description": "React text renderer",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuzik00/react-txt-renderer"
  },
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o storybook-static",
    "build": "ttsc --incremental",
    "test:code:lint": "yarn lint",
    "test:packages:audit": "audit-ci --config .audit-ci.json",
    "test": "run-p test:**",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "files": [
    "dist",
    "!dist/**/stories",
    "!dist/*.tsbuildinfo"
  ],
  "packageManager": "yarn@1.22.19",
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.2",
    "react-markdown-remark-directive": "^1.1.1",
    "react-outside-click-handler": "^1.3.0",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "remark-supersub": "^1.0.0",
    "remarkable": "^2.0.1",
    "unist-util-visit": "^4.1.0",
    "use-debounce": "^6.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/theming": "^6.5.10",
    "@types/draft-js": "^0.10.44",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.17",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/remarkable": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "audit-ci": "^6.2.0",
    "esbuild-loader": "^2.20.0",
    "eslint": "^7.3.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "husky": "^7.0.0",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.9",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "storybook-addon-live-examples": "^0.0.25",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.65.0"
  }
}
